{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "pages/index.tsx", "pages/_app.tsx", "pages/components/nav/assets/search/index.jsx", "pages/components/breadcrumb/index.tsx", "pages/Base/sidebar.tsx", "pages/Base/Pagelayout.tsx", "pages/components/nav/index.tsx", "pages/main/draft.tsx", "pages/main/homepage.tsx"],
  "exclude": ["node_modules"]
}
